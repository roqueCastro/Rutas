<resources>

    <!-- Base application theme. Theme.AppCompat.Light -->
    <style name="AppTheme" parent="Theme.AppCompat.Light">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/Azul_baique</item>
    </style>

    <style name="AppThemeBlue" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/Azul_baique</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/Azul_baique</item>
    </style>

    <style name="AppThemeGreen" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/verde_baique</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/verde_baique</item>
    </style>

    <style name="AppCompatAlertDialogStyle" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="colorAccent">@color/colorPrimaryDark</item>
        <item name="android:textColorPrimary">@color/design_default_color_primary_dark</item>
        <item name="android:background">@color/White</item>
    </style>

    <style name="RippleStyleBlack">
        <!-- Customize your theme here. -->
        <item name="mrl_rippleColor">#8096989A</item>
    </style>


</resources>